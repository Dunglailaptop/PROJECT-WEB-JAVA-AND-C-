@model webtnonline.Models.monhoc

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layoutnew.cshtml";
}



@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal"  style="margin-left:200px;margin-right:200px;">
    
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.monhoc_id)

        <div class="form-group">
           <h5>Tên môn học</h5>
            <div class="col-md-10">
                @Html.EditorFor(model => model.tenmonhoc, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.tenmonhoc, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
                |@Html.ActionLink("Back to List", "Index")
            </div>
        </div>
    </div>
}

