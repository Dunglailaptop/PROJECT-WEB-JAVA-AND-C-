@model webtnonline.Models.khoa

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layoutnew.cshtml";
}



@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal" style="margin-left:200px; margin-right:200px;">
        <h4>khóa</h4>
        
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.khoa_id)

        <div class="form-group">
            <h5>Tên khóa</h5>
            <div class="col-md-10">
                @Html.EditorFor(model => model.tenkhoa, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.tenkhoa, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />|
                @Html.ActionLink("Back to List", "Index")
            </div>
        </div>
    </div>
}

